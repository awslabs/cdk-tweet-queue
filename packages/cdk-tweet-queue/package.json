{
  "name": "cdk-tweet-queue",
  "version": "1.0.3",
  "description": "Defines an SQS queue with tweet stream from a search",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "/bin/bash ./prepare.sh && jsii",
    "watch": "jsii -w",
    "package": "jsii-pacmak && mkdir -p ../../dist && rsync -av dist/ ../../dist/",
    "test": "jest",
    "integ": "cdk -a 'node test/integ-test.js'"
  },
  "jsii": {
    "outdir": "dist",
    "targets": {
      "dotnet": {
        "namespace": "Eladb.CdkTweetQueue",
        "packageId": "Eladb.CdkTweetQueue",
        "signAssembly": false
      },
      "python": {
        "distName": "cdk-tweet-queue",
        "module": "cdk_tweet_queue"
      },
      "java": {
        "package": "com.github.eladb.tweetqueue",
        "maven": {
          "groupId": "com.github.eladb",
          "artifactId": "cdk-tweet-queue"
        }
      }
    }
  },
  "keywords": [
    "cdk",
    "aws-cdk",
    "twitter",
    "constructs"
  ],
  "author": {
    "name": "Elad Ben-Israel",
    "email": "elad.benisrael@gmail.com",
    "url": "https://twitter.com/emeshbi"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/eladb/cdk-tweet-queue.git"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@aws-cdk/aws-dynamodb": "^1.94.1",
    "@aws-cdk/aws-events": "^1.94.1",
    "@aws-cdk/aws-events-targets": "^1.94.1",
    "@aws-cdk/aws-iam": "^1.94.1",
    "@aws-cdk/aws-lambda": "^1.94.1",
    "@aws-cdk/aws-sqs": "^1.94.1",
    "@aws-cdk/core": "^1.94.1"
  },
  "devDependencies": {
    "@aws-cdk/assert": "^1.94.1",
    "@types/jest": "^26.0.0",
    "@types/node": "^13.7.1",
    "cdk-tweet-queue-poller": "^1.0.0",
    "jest": "^26.0.0",
    "jsii": "^1.25.0",
    "jsii-pacmak": "^1.25.0",
    "typescript": "^3.7.5"
  },
  "peerDependencies": {
    "@aws-cdk/aws-dynamodb": "^1.94.1",
    "@aws-cdk/aws-events": "^1.94.1",
    "@aws-cdk/aws-events-targets": "^1.94.1",
    "@aws-cdk/aws-iam": "^1.94.1",
    "@aws-cdk/aws-lambda": "^1.94.1",
    "@aws-cdk/aws-sqs": "^1.94.1",
    "@aws-cdk/core": "^1.94.1"
  }
}
